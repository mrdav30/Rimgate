<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <GeneCategoryDef>
        <defName>Rimgate_JaffaGenes</defName>
        <label>Goa'uld</label>
        <displayPriorityInXenotype>1101</displayPriorityInXenotype>
    </GeneCategoryDef>

    <GeneDef Name="Rimgate_JaffaGenesBase" Abstract="True">
        <displayCategory>Rimgate_JaffaGenes</displayCategory>
        <canGenerateInGeneSet>false</canGenerateInGeneSet>
    </GeneDef>

    <GeneDef ParentName="Rimgate_JaffaGenesBase">
        <defName>Rimgate_JaffaLongevity</defName>
        <label>life longevity</label>
        <description>This gene drastically slows aging and extends the host’s prime. Perfect for long campaigns, ceremonial posturing, and outliving your enemies...</description>
        <iconPath>UI/Icon/Gene/RGJaffaLongevityGeneIcon</iconPath>
        <displayCategory>Archite</displayCategory>
        <statFactors>
            <LearningRateFactor>0.40</LearningRateFactor>
        </statFactors>
        <biologicalAgeTickFactorFromAgeCurve>
            <points>
                <li>(13, 0.25)</li>
                <li>(18, 0.25)</li>
            </points>
        </biologicalAgeTickFactorFromAgeCurve>
        <modExtensions>
            <li Class="VEF.Genes.GeneExtension">
                <backgroundPathEndogenes>UI/Icon/Gene/RGEndogenesIcon</backgroundPathEndogenes>
                <backgroundPathXenogenes>UI/Icon/Gene/RGXenogenes</backgroundPathXenogenes>
            </li>
        </modExtensions>
    </GeneDef>

    <GeneDef ParentName="Rimgate_JaffaGenesBase">
        <defName>Rimgate_SymbiotePouch</defName>
        <label>symbiote pouch</label>
        <description>This gene rewrites the host’s anatomy to form a specialized abdominal pouch. Once considered a grotesque mark of servitude to the Goa’uld, it is also a symbol of power—granting the bearer the ability to house a living symbiote within. Without a symbiote, the pouch lies dormant, a hollow reminder of dependency.</description>
        <iconPath>UI/Icon/Gene/RGJaffaLongevityGeneIcon</iconPath>
        <displayCategory>Archite</displayCategory>
        <modExtensions>
            <li Class="VEF.Genes.GeneExtension">
                <backgroundPathEndogenes>UI/Icon/Gene/RGEndogenesIcon</backgroundPathEndogenes>
                <backgroundPathXenogenes>UI/Icon/Gene/RGXenogenes</backgroundPathXenogenes>
                <hediffsToBodyParts>
                    <li>
                        <hediff>Rimgate_SymbiotePouch</hediff>
                        <bodyparts>
                            <li>Stomach</li>
                        </bodyparts>
                    </li>
                </hediffsToBodyParts>
            </li>
        </modExtensions>
    </GeneDef>

    <GeneDef ParentName="Rimgate_JaffaGenesBase">
        <defName>Rimgate_JaffaCombatDiscipline</defName>
        <label>combat discipline</label>
        <description>Bred for war and obedience, carriers of this gene exhibit elite combat reflexes and tactical focus. Also exhibit zero interest in art, chit-chat, or anything remotely civilian. They didn’t come here to talk.</description>
        <iconPath>UI/Icon/Gene/RGJaffaDisciplineGeneIcon</iconPath>
        <suppressedTraits>
            <li>
                <def>Wimp</def>
            </li>
        </suppressedTraits>
        <biostatMet>-6</biostatMet>
        <biostatCpx>3</biostatCpx>
        <displayCategory>Violence</displayCategory>
        <statFactors>
            <AimingDelayFactor>0.9</AimingDelayFactor>
        </statFactors>
        <statOffsets>
            <MeleeHitChance>4</MeleeHitChance>
            <MeleeDodgeChance>15</MeleeDodgeChance>
            <ShootingAccuracyPawn>6</ShootingAccuracyPawn>
        </statOffsets>
        <disabledWorkTags>
            <li>Caring</li>
            <li>Mining</li>
            <li>Artistic</li>
            <li>Social</li>
            <li>Cleaning</li>
        </disabledWorkTags>
        <modExtensions>
            <li Class="VEF.Genes.GeneExtension">
                <backgroundPathEndogenes>UI/Icon/Gene/RGEndogenesIcon</backgroundPathEndogenes>
                <backgroundPathXenogenes>UI/Icon/Gene/RGXenogenes</backgroundPathXenogenes>
            </li>
        </modExtensions>
    </GeneDef>

    <GeneDef ParentName="Rimgate_JaffaGenesBase">
        <defName>Rimgate_JaffaWarrior</defName>
        <label>jaffa warrior</label>
        <description>This gene reinforces mental stability and pain resistance, ensuring the warrior stays upright, angry, and terrifyingly calm under fire. Mental breaks are for lesser species.</description>
        <iconPath>UI/Icon/Gene/RGJaffaWarriorGeneIcon</iconPath>
        <biostatMet>-6</biostatMet>
        <displayCategory>Violence</displayCategory>
        <statFactors>
            <HuntingStealth>1.25</HuntingStealth>
            <PilotingAbility>2</PilotingAbility>
        </statFactors>
        <statOffsets>
            <PainShockThreshold>0.5</PainShockThreshold>
            <MentalBreakThreshold>-0.19</MentalBreakThreshold>
            <ToxicEnvironmentResistance>0.5</ToxicEnvironmentResistance>
        </statOffsets>
        <symbolPack>
            <prefixSymbols>
                <li>
                    <symbol>aggro</symbol>
                </li>
                <li>
                    <symbol>fight</symbol>
                </li>
                <li>
                    <symbol>barb</symbol>
                </li>
                <li>
                    <symbol>sturdy</symbol>
                </li>
                <li>
                    <symbol>stout</symbol>
                </li>
            </prefixSymbols>
            <suffixSymbols>
                <li>
                    <symbol>stub</symbol>
                </li>
                <li>
                    <symbol>fighter</symbol>
                </li>
            </suffixSymbols>
        </symbolPack>
        <modExtensions>
            <li Class="VEF.Genes.GeneExtension">
                <backgroundPathEndogenes>UI/Icon/Gene/RGEndogenesIcon</backgroundPathEndogenes>
                <backgroundPathXenogenes>UI/Icon/Gene/RGXenogenes</backgroundPathXenogenes>
            </li>
        </modExtensions>
    </GeneDef>

</Defs>