<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- ==================== Prim'ta ==================== -->

	<RecipeDef ParentName="SurgeryRemoveImplantBase">
		<defName>Rimgate_RemovePrimtaSymbiote</defName>
		<label>remove Prim'ta symbiote</label>
		<description>Carefully extract a Prim'ta symbiote from the host's pouch. If the Jaffa does not receive another Prim'ta—or an artificial replacement like tretonin—their body will begin to fail, spiraling into symbiote withdrawal.</description>
		<descriptionHyperlinks>
			<HediffDef>Rimgate_SymbioteWithdrawal</HediffDef>
		</descriptionHyperlinks>
		<jobString>removing Prim'ta symbiote.</jobString>
		<effectWorking>Surgery</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workSpeedStat>MedicalOperationSpeed</workSpeedStat>
		<workSkill>Medicine</workSkill>
		<workSkillLearnFactor>16</workSkillLearnFactor>
		<workAmount>2000</workAmount>
		<anesthetize>true</anesthetize>
		<skillRequirements>
			<Medicine>15</Medicine>
		</skillRequirements>
		<surgerySuccessChanceFactor>0.5</surgerySuccessChanceFactor>
		<removesHediff>Rimgate_PrimtaInPouch</removesHediff>
		<researchPrerequisite>Rimgate_SymbioteStudy</researchPrerequisite>
	</RecipeDef>

