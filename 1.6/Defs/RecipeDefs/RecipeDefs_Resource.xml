<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- ==================== Naquadria ==================== -->

	<RecipeDef>
		<defName>Rimgate_MakeNaquadria</defName>
		<label>refine Naquadria</label>
		<description>make a batch of Naquadria from Naquadah.</description>
		<jobString>making Naquadria.</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>2000</workAmount>
		<workSpeedStat>SmeltingSpeed</workSpeedStat>
		<recipeUsers>
			<li>FabricationBench</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Rimgate_Naquadah</li>
					</thingDefs>
				</filter>
				<count>15</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Rimgate_Naquadah</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Crafting</workSkill>
		<products>
			<Rimgate_Naquadria>5</Rimgate_Naquadria>
		</products>
		<researchPrerequisite>Rimgate_NaquadahRefinement</researchPrerequisite>
	</RecipeDef>

	<!-- ==================== Tretonin ==================== -->

	<RecipeDef>
		<defName>Rimgate_MakeTretonin</defName>
		<label>refine Tretonin</label>
		<description>make a batch of Tretonin from a Goa'uld symbiote.</description>
		<jobString>making Tretonin.</jobString>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<workAmount>2000</workAmount>
		<workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
		<recipeUsers>
			<li>DrugLab</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Rimgate_SymbioteImplant</li>
						<li>Rimgate_Naquadah</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Rimgate_SymbioteImplant</li>
				<li>Rimgate_Naquadah</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Crafting</workSkill>
		<products>
			<Rimgate_Tretonin>5</Rimgate_Tretonin>
		</products>
		<researchPrerequisite>Rimgate_TretoninSynthesis</researchPrerequisite>
	</RecipeDef>

	<!-- ==================== Iris Stuff ==================== -->

	<RecipeDef>
		<defName>Rimgate_MakeIrisInstallKit</defName>
		<label>make Iris installation kit</label>
		<description>make an Iris installation kit.</description>
		<jobString>making Iris installation kit.</jobString>
		<workAmount>10000</workAmount>
		<recipeUsers>
			<li>FabricationBench</li>
		</recipeUsers>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<skillRequirements>
			<Crafting>10</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
		<unfinishedThingDef>Rimgate_UnfinishedIrisInstallKit</unfinishedThingDef>
		<recipeUsers>
			<li>FabricationBench</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>250</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Rimgate_Naquadah</li>
					</thingDefs>
				</filter>
				<count>150</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Uranium</li>
					</thingDefs>
				</filter>
				<count>300</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>200</count>
			</li>
		</ingredients>
		<products>
			<Rimgate_IrisInstallKit>1</Rimgate_IrisInstallKit>
		</products>
		<researchPrerequisite>Rimgate_StargateIrisResearch</researchPrerequisite>
	</RecipeDef>

