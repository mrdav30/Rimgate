	<!-- ==================== Biomaterial Procurement Kit ==================== -->

	<RecipeDef>
		<defName>Rimgate_MakeBiomaterialProcurementKit</defName>
		<label>assemble biomaterial procurement kit</label>
		<description>Assemble a biomaterial procurement kit.</description>

		<jobString>Assembling biomaterial procurement kit.</jobString>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>

		<workAmount>1800</workAmount>
		<workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.03</workSkillLearnFactor>

		<skillRequirements>
			<Crafting>6</Crafting>
			<Medicine>4</Medicine>
		</skillRequirements>

		<recipeUsers>
			<li>DrugLab</li>
		</recipeUsers>

		<ingredients>
			<!-- sterile supplies -->
			<li>
				<filter>
					<thingDefs>
						<li>MedicineIndustrial</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>

			<!-- containers + tools -->
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>15</count>
			</li>

			<li>
				<filter>
					<thingDefs>
						<li>Cloth</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>

			<!-- “preservative gel” stand-in -->
			<li>
				<filter>
					<thingDefs>
						<li>Chemfuel</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>

			<!-- optional: make it feel more spacer/medical -->
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>

		<!-- Produce multiple to reduce micromanagement -->
		<products>
			<Rimgate_BiomaterialProcurementKit>3</Rimgate_BiomaterialProcurementKit>
		</products>

		<researchPrerequisite>Bionics</researchPrerequisite>
	</RecipeDef>

</Defs>