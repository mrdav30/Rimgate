<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- ==================== Goa'uld Symbiote ==================== -->

	<RecipeDef ParentName="SurgeryInstallImplantBase">
		<defName>Rimgate_InstallSymbiote</defName>
		<label>Install symbiote</label>
		<description>Implant a Goa'uld symbiote into the host, granting enhanced health, strength, and regenerative properties.</description>
		<descriptionHyperlinks>
			<ThingDef>Rimgate_SymbioteImplant</ThingDef>
			<HediffDef>Rimgate_SymbioteImplant</HediffDef>
		</descriptionHyperlinks>
		<jobString>Implanting symbiote.</jobString>
		<workAmount>2500</workAmount>
		<workSpeedStat>MedicalOperationSpeed</workSpeedStat>
		<effectWorking>Surgery</effectWorking>
		<soundWorking>Recipe_Surgery</soundWorking>
		<surgerySuccessChanceFactor>1.0</surgerySuccessChanceFactor>
		<addsHediff>Rimgate_SymbioteImplant</addsHediff>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>Rimgate_SymbioteImplant</li>
			</thingDefs>
		</fixedIngredientFilter>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Rimgate_SymbioteImplant</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<appliedOnFixedBodyParts>
			<li>Stomach</li>
		</appliedOnFixedBodyParts>
		<targetsBodyPart>true</targetsBodyPart>
		<anesthetize>true</anesthetize>
		<skillRequirements>
			<Medicine>5</Medicine>
		</skillRequirements>
		<addsHediff>Rimgate_SymbioteImplant</addsHediff>
	</RecipeDef>

	<RecipeDef ParentName="SurgeryRemoveImplantBase">
		<defName>Rimgate_RemoveGoauldSymbiote</defName>
		<label>remove Goa'uld symbiote</label>
		<description>Remove a Goa'uld symbiote.</description>
		<jobString>Removing Goa'uld symbiote.</jobString>
		<effectWorking>Surgery</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workSpeedStat>MedicalOperationSpeed</workSpeedStat>
		<workSkill>Medicine</workSkill>
		<workSkillLearnFactor>16</workSkillLearnFactor>
		<workAmount>2000</workAmount>
		<anesthetize>true</anesthetize>
		<skillRequirements>
			<Medicine>15</Medicine>
		</skillRequirements>
		<surgerySuccessChanceFactor>0.5</surgerySuccessChanceFactor>
		<addsHediff>Rimgate_SymbioteWithdrawal</addsHediff>
	</RecipeDef>

