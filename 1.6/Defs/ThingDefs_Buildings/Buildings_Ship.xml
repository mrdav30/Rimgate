      <moteSpawnTime>0.05</moteSpawnTime>
      <shadow>Things/Ship/Teltak/RGGoauldTeltakShadow</shadow>
      <shadowSize>(7,8)</shadowSize>
      <motesPerCell>1</motesPerCell>
      <rotationCurve>
        <points>
          <li>(0,0)</li>
          <li>(0.15,12)</li>
          <li>(0.5,-4)</li>
        </points>
      </rotationCurve>
      <zPositionCurve>
        <points>
          <li>(0,0)</li>
          <li>(0.08,2)</li>
        </points>
      </zPositionCurve>
      <speedCurve>
        <points>
          <li>(0,0.25)</li>
          <li>(0.4,0.9)</li>
        </points>
      </speedCurve>
    </skyfaller>
  </ThingDef>

