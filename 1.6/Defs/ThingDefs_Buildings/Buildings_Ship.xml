  <!-- =============== Tauri Puddle Jumper =============== -->

  <ThingDef ParentName="BuildingBase">
    <defName>Rimgate_TauriPuddleJumper</defName>
    <label>Tau'ri Puddle Jumper</label>
    <description>a marvel of Ancient engineering, retrofitted by the Tau'ri. Compact, shielded, and packed with tech most civilizations would kill for—literally. Perfect for stealth ops, last-minute escapes, or joyrides through wormholes.</description>
    <thingClass>Building_PassengerShuttle</thingClass>
    <size>(4,6)</size>
    <graphicData>
      <texPath>Things/Ship/PuddleJumper/RGTauriPuddleJumper</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(5,6)</drawSize>
      <shadowData>
        <volume>(0.8, 0.6, 0.8)</volume>
      </shadowData>
    </graphicData>
    <!-- <uiIconPath>Things/Ship/PuddleJumper/RGTauriPuddleJumper</uiIconPath> -->
    <uiIconScale>0.9</uiIconScale>
    <defaultPlacingRot>East</defaultPlacingRot>
    <tickerType>Normal</tickerType>
    <fillPercent>0.4</fillPercent>
    <pathCost>50</pathCost>
    <drawOffscreen>true</drawOffscreen>
    <passability>PassThroughOnly</passability>
    <stealable>false</stealable>
    <altitudeLayer>Building</altitudeLayer>
    <receivesSignals>true</receivesSignals>
    <neverMultiSelect>true</neverMultiSelect>
    <drawerType>MapMeshAndRealTime</drawerType>
    <statBases>
      <MaxHitPoints>3000</MaxHitPoints>
      <WorkToBuild>24000</WorkToBuild>
      <Mass>114514</Mass>
      <Flammability>0.5</Flammability>
    </statBases>
    <designationCategory>Odyssey</designationCategory>
    <costList>
      <Steel>750</Steel>
      <Plasteel>250</Plasteel>
      <ComponentIndustrial>16</ComponentIndustrial>
      <ComponentSpacer>15</ComponentSpacer>
      <Rimgate_Naquadah>75</Rimgate_Naquadah>
    </costList>
    <comps>
      <li Class="Rimgate.CompProperties_ShieldEmitter">
        <compClass>Rimgate.Comp_ShieldEmitter</compClass>
        <interceptAirProjectiles>true</interceptAirProjectiles>
        <interceptGroundProjectiles>true</interceptGroundProjectiles>
        <podBlocker>false</podBlocker>
        <resetTime>4000</resetTime>
        <fuelConsumptionRate>0.05</fuelConsumptionRate>
        <stressPerDamage>0.02</stressPerDamage>
        <empDamageFactor>4.5</empDamageFactor>
        <shieldOverloadThreshold>0.9</shieldOverloadThreshold>
        <shieldOverloadChance>0.05</shieldOverloadChance>
        <extraOverloadRange>10</extraOverloadRange>
        <overloadDamageType>EMP</overloadDamageType>
        <explodeOnCollapse>false</explodeOnCollapse>
        <shieldCanBeScaled>true</shieldCanBeScaled>
        <shieldScaleLimits>10~20 </shieldScaleLimits>
        <shieldScaleDefault>10</shieldScaleDefault>
        <shieldColour>(237, 227, 133)</shieldColour>
      </li>
      <li Class="CompProperties_Shuttle">
        <shipDef>Rimgate_TauriPuddleJumperShip</shipDef>
      </li>
      <li Class="CompProperties_Launchable">
        <fuelPerTile>2.8</fuelPerTile>
        <minFuelCost>65</minFuelCost>
        <skyfallerLeaving>Rimgate_PuddleJumperLeaving</skyfallerLeaving>
        <worldObjectDef>Rimgate_TauriPuddleJumper</worldObjectDef>
        <cooldownTicks>3000</cooldownTicks>
        <fixedLaunchDistanceMax>70</fixedLaunchDistanceMax>
      </li>
      <li Class="CompProperties_Transporter">
        <massCapacity>4400</massCapacity>
        <showMassInInspectString>true</showMassInInspectString>
      </li>
      <li Class="CompProperties_Refuelable">
        <fuelCapacity>2500</fuelCapacity>
        <targetFuelLevelConfigurable>true</targetFuelLevelConfigurable>
        <initialConfigurableTargetFuelLevel>2500</initialConfigurableTargetFuelLevel>
        <fuelFilter>
          <thingDefs>
            <li>Rimgate_Naquadria</li>
          </thingDefs>
        </fuelFilter>
        <initialFuelPercent>0.25</initialFuelPercent>
        <consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
        <autoRefuelPercent>1</autoRefuelPercent>
        <showFuelGizmo>true</showFuelGizmo>
      </li>
      <li Class="Rimgate.CompProperties_Toggle">
        <compClass>Rimgate.Comp_Toggle</compClass>
        <commandLabelKey>RG_ShieldToggleLabel</commandLabelKey>
        <commandDescKey>RG_ShieldToggleDescription</commandDescKey>
      </li>
    </comps>
    <inspectorTabs>
      <li>ITab_ContentsTransporter</li>
    </inspectorTabs>
    <researchPrerequisites>
      <li>Rimgate_TauriPuddleJumper</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>10</constructionSkillPrerequisite>
  </ThingDef>

