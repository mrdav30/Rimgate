<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- =============== Tel'tak =============== -->

  <ThingDef ParentName="BuildingBase">
    <defName>Rimgate_GoauldTeltak</defName>
    <label>Goa'uld Tel'tak</label>
    <description>a compact Goa'uld transport craft built for swift infiltration, planetary landings, and discreet extractions. Lacks firepower—but makes up for it with sheer speed, stealth plating, and enough ego to carry a system lord.</description>
    <thingClass>Building_PassengerShuttle</thingClass>
    <preventDroppingThingsOn>true</preventDroppingThingsOn>
    <size>(5,6)</size>
    <graphicData>
      <texPath>Things/Ship/Teltak/RGGoauldTeltak</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(6,6)</drawSize>
      <shadowData>
        <volume>(0.8, 0.6, 0.8)</volume>
      </shadowData>
    </graphicData>
    <defaultPlacingRot>East</defaultPlacingRot>
    <pathCost>50</pathCost>
    <drawOffscreen>true</drawOffscreen>
    <passability>PassThroughOnly</passability>
    <stealable>false</stealable>
    <altitudeLayer>Building</altitudeLayer>
    <receivesSignals>true</receivesSignals>
    <neverMultiSelect>true</neverMultiSelect>
    <statBases>
      <MaxHitPoints>4400</MaxHitPoints>
      <WorkToBuild>24000</WorkToBuild>
      <Mass>450</Mass>
      <Flammability>0.5</Flammability>
      <Comfort>0.75</Comfort>
    </statBases>
    <hasInteractionCell>true</hasInteractionCell>
    <interactionCellOffset>(4, 0, 0)</interactionCellOffset>
    <selectable>true</selectable>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <preventSkyfallersLandingOn>true</preventSkyfallersLandingOn>
    <drawerType>RealtimeOnly</drawerType>
    <repairEffect>ConstructMetal</repairEffect>
    <building>
      <claimable>false</claimable>
      <destroySound>BuildingDestroyed_Metal_Big</destroySound>
      <isInert>true</isInert>
    </building>
    <designationCategory>Odyssey</designationCategory>
    <costList>
      <Steel>1000</Steel>
      <Gold>200</Gold>
      <Plasteel>250</Plasteel>
      <ComponentIndustrial>16</ComponentIndustrial>
      <ComponentSpacer>5</ComponentSpacer>
      <Rimgate_Naquadah>100</Rimgate_Naquadah>
    </costList>
    <comps>
      <li Class="CompProperties_Shuttle">
        <shipDef>Rimgate_GoauldTeltakShip</shipDef>
      </li>
      <li Class="CompProperties_Launchable">
        <fuelPerTile>4.0</fuelPerTile>
        <minFuelCost>75</minFuelCost>
        <skyfallerLeaving>Rimgate_TeltakLeaving</skyfallerLeaving>
        <worldObjectDef>Rimgate_GoauldTeltak</worldObjectDef>
        <cooldownTicks>4000</cooldownTicks>
        <fixedLaunchDistanceMax>120</fixedLaunchDistanceMax>
        <cooldownEndedMessage>{0} is ready to launch again.</cooldownEndedMessage>
      </li>
      <li Class="CompProperties_Transporter">
        <massCapacity>2200</massCapacity>
        <max1PerGroup>true</max1PerGroup>
        <canChangeAssignedThingsAfterStarting>true</canChangeAssignedThingsAfterStarting>
        <pawnLoadedSound>Shuttle_PawnLoaded</pawnLoadedSound>
        <pawnExitSound>Shuttle_PawnExit</pawnExitSound>
        <showMassInInspectString>true</showMassInInspectString>
      </li>
      <li Class="CompProperties_Refuelable">
        <fuelCapacity>3000</fuelCapacity>
        <targetFuelLevelConfigurable>true</targetFuelLevelConfigurable>
        <initialConfigurableTargetFuelLevel>3000</initialConfigurableTargetFuelLevel>
        <fuelFilter>
          <thingDefs>
            <li>Rimgate_Naquadria</li>
          </thingDefs>
        </fuelFilter>
        <initialFuelPercent>0.25</initialFuelPercent>
        <consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
        <autoRefuelPercent>1</autoRefuelPercent>
        <showFuelGizmo>true</showFuelGizmo>
        <drawOutOfFuelOverlay>false</drawOutOfFuelOverlay>
        <canEjectFuel>true</canEjectFuel>
      </li>
    </comps>
    <inspectorTabs>
      <li>ITab_ContentsTransporter</li>
    </inspectorTabs>
    <placeWorkers>
      <li>PlaceWorker_NotUnderRoof</li>
    </placeWorkers>
    <researchPrerequisites>
      <li>Rimgate_GoauldTelTak</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
  </ThingDef>

  <ThingDef ParentName="ShuttleSkyfallerBase">
    <defName>Rimgate_TeltakIncoming</defName>
    <label>Goa'uld Tel'tak (incoming)</label>
    <thingClass>PassengerShuttleIncoming</thingClass>
    <graphicData>
      <graphicClass>Graphic_Multi</graphicClass>
      <texPath>Things/Ship/Teltak/RGGoauldTeltak</texPath>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(6,6)</drawSize>
    </graphicData>
    <skyfaller>
      <anticipationSound>Shuttle_Landing</anticipationSound>
      <anticipationSoundTicks>100</anticipationSoundTicks>
      <ticksToImpactRange>160~200</ticksToImpactRange>
      <shadowSize>(6.5,6.5)</shadowSize>
      <shadow>Things/Ship/Teltak/RGGoauldTeltakShadow</shadow>
      <rotationCurve>
        <points>
          <li>(0,25)</li>
          <li>(0.5,4)</li>
          <li>(0.9,-4)</li>
          <li>(0.95,0)</li>
        </points>
      </rotationCurve>
      <zPositionCurve>
        <points>
          <li>(0.95,2.5)</li>
          <li>(1,0)</li>
        </points>
      </zPositionCurve>
      <speedCurve>
        <points>
          <li>(0.6,0.8)</li>
          <li>(0.95,0.12)</li>
        </points>
      </speedCurve>
    </skyfaller>
  </ThingDef>

  <ThingDef ParentName="ShuttleSkyfallerBase">
    <defName>Rimgate_TeltakLeaving</defName>
    <label>Goa'uld Tel'tak (leaving)</label>
    <thingClass>PassengerShuttleLeaving</thingClass>
    <rotatable>true</rotatable>
    <graphicData>
      <graphicClass>Graphic_Multi</graphicClass>
      <texPath>Things/Ship/Teltak/RGGoauldTeltak</texPath>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(6,6)</drawSize>
    </graphicData>
    <skyfaller>
      <reversed>true</reversed>
      <anticipationSound>Shuttle_Leaving</anticipationSound>
      <anticipationSoundTicks>-10</anticipationSoundTicks>
      <ticksToImpactRange>-40~-15</ticksToImpactRange>
      <moteSpawnTime>0.05</moteSpawnTime>
      <shadow>Things/Ship/Teltak/RGGoauldTeltakShadow</shadow>
      <shadowSize>(6.5,6.5)</shadowSize>
      <motesPerCell>1</motesPerCell>
      <rotationCurve>
        <points>
          <li>(0,0)</li>
          <li>(0.15,12)</li>
          <li>(0.5,-4)</li>
        </points>
      </rotationCurve>
      <zPositionCurve>
        <points>
          <li>(0,0)</li>
          <li>(0.08,2)</li>
        </points>
      </zPositionCurve>
      <speedCurve>
        <points>
          <li>(0,0.25)</li>
          <li>(0.4,0.9)</li>
        </points>
      </speedCurve>
    </skyfaller>
  </ThingDef>

