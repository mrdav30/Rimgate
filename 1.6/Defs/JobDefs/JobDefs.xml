<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- Sarcophagus -->

	<JobDef>
		<defName>Rimgate_CarryToSarcophagus</defName>
		<driverClass>Rimgate.JobDriver_CarryToSarcophagus</driverClass>
		<reportString>carrying TargetA to TargetB.</reportString>
		<casualInterruptible>false</casualInterruptible>
		<carryThingAfterJob>true</carryThingAfterJob>
	</JobDef>

	<JobDef>
		<defName>Rimgate_PatientGoToSarcophagus</defName>
		<driverClass>Rimgate.JobDriver_PatientGoToSarcophagus</driverClass>
		<reportString>receiving treatment in TargetA.</reportString>
		<suspendable>false</suspendable>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_RescueToSarcophagus</defName>
		<driverClass>Rimgate.JobDriver_CarryToSarcophagus</driverClass>
		<reportString>rescuing TargetA to TargetB.</reportString>
		<casualInterruptible>false</casualInterruptible>
		<carryThingAfterJob>true</carryThingAfterJob>
	</JobDef>

	<!-- Stargate -->

	<JobDef>
		<defName>Rimgate_EnterStargate</defName>
		<driverClass>Rimgate.JobDriver_EnterStargate</driverClass>
		<reportString>Entering TargetA.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_DialStargate</defName>
		<driverClass>Rimgate.JobDriver_DialStargate</driverClass>
		<reportString>Using TargetA.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_BringToStargate</defName>
		<driverClass>Rimgate.JobDriver_BringToStargate</driverClass>
		<reportString>Bringing TargetA to TargetB.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_InstallIris</defName>
		<driverClass>Rimgate.JobDriver_InstallIris</driverClass>
		<reportString>Installing iris on TargetB.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_DecodeGlyphs</defName>
		<driverClass>Rimgate.JobDriver_DecodeGlyphs</driverClass>
		<reportString>Decoding stargate glyphs.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<!-- Misc -->

	<JobDef>
		<defName>Rimgate_Toggle</defName>
		<driverClass>Rimgate.JobDriver_Toggle</driverClass>
		<reportString>toggling switch.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

</Defs>