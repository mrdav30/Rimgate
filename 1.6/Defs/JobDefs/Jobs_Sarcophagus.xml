<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<JobDef>
		<defName>Rimgate_CarryToSarcophagus</defName>
		<driverClass>JobDriver_TakeToBed</driverClass>
		<reportString>carrying TargetA to TargetB.</reportString>
		<casualInterruptible>false</casualInterruptible>
		<carryThingAfterJob>true</carryThingAfterJob>
	</JobDef>
	
	<JobDef>
		<defName>Rimgate_PatientGoToSarcophagus</defName>
		<driverClass>Rimgate.JobDriver_PatientGoToSarcophagus</driverClass>
		<reportString>receiving treatment in TargetA.</reportString>
		<suspendable>false</suspendable>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_RescueToSarcophagus</defName>
		<driverClass>JobDriver_TakeToBed</driverClass>
		<reportString>rescuing TargetA to TargetB.</reportString>
		<casualInterruptible>false</casualInterruptible>
		<carryThingAfterJob>true</carryThingAfterJob>
	</JobDef>

</Defs>