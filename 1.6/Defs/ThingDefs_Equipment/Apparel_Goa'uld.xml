	<!-- ====== Goa'uld Armor ====== -->

	<ThingDef Name="Rimgate_JaffaArmorBase" ParentName="ArmorSmithableBase">
		<defName>Rimgate_JaffaArmor</defName>
		<label>Jaffa armor</label>
		<description>Standard-issue Jaffa power armor: bulky, impressive, and likely to protect everything except your feelings. Consists of a chainmail tunic with attaching chest and back plates. \n\nBonus points if you shout “Shol'va!” mid-charge.</description>
		<techLevel>Spacer</techLevel>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<defaultStuff>Steel</defaultStuff>
		<costStuffCount>60</costStuffCount>
		<costList>
			<Steel>60</Steel>
			<Rimgate_Naquadah>25</Rimgate_Naquadah>
		</costList>
		<colorPerStuff>
			<li>
				<stuff>Steel</stuff>
				<color>(1, 1, 1, 1)</color>
			</li>
		</colorPerStuff>
		<graphicData>
			<texPath>Things/Item/Apparel/Torso/Jaffa/RGJaffaArmorIcon</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<colorGenerator IsNull="True" />
		<recipeMaker>
			<researchPrerequisite>Rimgate_GoauldArmorTierOne</researchPrerequisite>
			<soundWorking>Recipe_Machining</soundWorking>
			<skillRequirements>
				<Crafting>6</Crafting>
			</skillRequirements>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
			<defaultIngredientFilter>
				<disallowedThingDefs>
					<li>Rimgate_Naquadah</li>
				</disallowedThingDefs>
			</defaultIngredientFilter>
		</recipeMaker>
		<statBases>
			<MarketValue>750</MarketValue>
			<MaxHitPoints>280</MaxHitPoints>
			<WorkToMake>12500</WorkToMake>
			<Mass>15</Mass>
			<StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
			<ArmorRating_Sharp>0.82</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.35</ArmorRating_Blunt>
			<ArmorRating_Heat>0.48</ArmorRating_Heat>
			<Insulation_Cold>35</Insulation_Cold>
			<Insulation_Heat>11</Insulation_Heat>
			<Flammability>0.4</Flammability>
			<EquipDelay>15</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.15</MoveSpeed>
			<MentalBreakThreshold>-0.10</MentalBreakThreshold>
			<PainShockThreshold>0.1</PainShockThreshold>
			<SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">-0.1</SlaveSuppressionOffset>
			<VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">0.3</VacuumResistance>
		</equippedStatOffsets>
		<apparel>
			<wornGraphicPath>Things/Item/Apparel/Torso/Jaffa/RGJaffaArmor</wornGraphicPath>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
			<useDeflectMetalEffect>true</useDeflectMetalEffect>
			<soundWear>Wear_PowerArmor</soundWear>
			<soundRemove>Remove_PowerArmor</soundRemove>
			<bodyPartGroups>
				<li>Neck</li>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<layers>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<tags>
				<li>Rimgate_GoauldApparel_tag</li>
				<li>Rimgate_GoauldGuardApparel_tag</li>
			</tags>
		</apparel>
		<tradeTags>
			<li>Rimgate_GoauldApparelTrade_tag</li>
			<li>HiTechArmor</li>
			<li>ExoticMisc</li>
		</tradeTags>
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<comps Inherit="False">
			<li Class="CompProperties_Forbiddable" />
			<li>
				<compClass>CompQuality</compClass>
			</li>
		</comps>
