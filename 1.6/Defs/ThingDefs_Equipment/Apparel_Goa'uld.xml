	<!-- ====== Kull Armor ====== -->

	<ThingDef ParentName="ArmorMachineableBase">
		<defName>Rimgate_KullArmor</defName>
		<label>Kull armor</label>
		<description>Heavy as a Goa’uld ego, this armor makes you practically bulletproof—and also the slowest thing on the map. Great for long walks... through enemy fire.</description>
		<techLevel>Ultra</techLevel>
		<costList>
			<Plasteel>70</Plasteel>
			<Cloth>50</Cloth>
			<Uranium>30</Uranium>
			<Synthread>25</Synthread>
			<Rimgate_Naquadah>15</Rimgate_Naquadah>
			<ComponentIndustrial>6</ComponentIndustrial>
			<ComponentSpacer>4</ComponentSpacer>
		</costList>
		<recipeMaker>
			<researchPrerequisite>Rimgate_KullArmor</researchPrerequisite>
			<recipeUsers>
				<li>FabricationBench</li>
			</recipeUsers>
			<skillRequirements>
				<Crafting>10</Crafting>
				<Intellectual>6</Intellectual>
			</skillRequirements>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
		</recipeMaker>
		<colorGenerator IsNull="True" />
		<graphicData>
			<texPath>Things/Item/Apparel/Torso/Kull/RGKullArmorIcon</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>2500</MarketValue>
			<MaxHitPoints>350</MaxHitPoints>
			<WorkToMake>12500</WorkToMake>
			<Mass>17</Mass>
			<Flammability>0.6</Flammability>
			<ArmorRating_Sharp>1.18</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.45</ArmorRating_Blunt>
			<ArmorRating_Heat>0.55</ArmorRating_Heat>
			<Insulation_Cold>35</Insulation_Cold>
			<Insulation_Heat>11</Insulation_Heat>
			<EquipDelay>20</EquipDelay>
			<EnergyShieldRechargeRate>0.013</EnergyShieldRechargeRate>
			<EnergyShieldEnergyMax>2.5</EnergyShieldEnergyMax>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.50</MoveSpeed>
			<PainShockThreshold>0.2</PainShockThreshold>
			<SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">-0.1</SlaveSuppressionOffset>
			<VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">0.3</VacuumResistance>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>Neck</li>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Hands</li>
				<li>Legs</li>
				<li>Feet</li>
			</bodyPartGroups>
			<layers>
				<li>OnSkin</li>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
			<useDeflectMetalEffect>true</useDeflectMetalEffect>
			<soundWear>Wear_PowerArmor</soundWear>
			<soundRemove>Remove_PowerArmor</soundRemove>
			<wornGraphicPath>Things/Item/Apparel/Torso/Kull/RGKullArmor</wornGraphicPath>
			<tags>
				<li>Rimgate_KullApparel_tag</li>
			</tags>
		</apparel>
		<tickerType>Normal</tickerType>
		<tradeTags Inherit="False" />
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<comps Inherit="False">
			<li Class="CompProperties_Forbiddable" />
			<li>
				<compClass>CompQuality</compClass>
			</li>
			<li Class="CompProperties_Shield">
				<startingTicksToReset>6400</startingTicksToReset><!-- 1 hour -->
				<minDrawSize>1.4</minDrawSize>
				<maxDrawSize>1.8</maxDrawSize>
				<energyLossPerDamage>0.02</energyLossPerDamage>
				<energyOnReset>2.5</energyOnReset>
				<blocksRangedWeapons>false</blocksRangedWeapons>
			</li>
			<li Class="Rimgate.CompProperties_AnalyzableResearchWhen">
				<analysisID>1000210</analysisID>
				<requiresResearchDef>ChargedShot</requiresResearchDef>

				<!-- Analyzable -->

				<destroyedOnAnalyzed>true</destroyedOnAnalyzed>
				<analysisDurationHours>6</analysisDurationHours>
				<completedLetterLabel>Kull armor studied: {RESEARCH} unlocked</completedLetterLabel>
				<completedLetter>By studying the Kull’s armor, {PAWN_labelShort} has acquired the insight needed for the research project(s) {RESEARCH}.\n\nThe composite plating reacted violently when its power matrix was breached, vaporizing most of the sample.\n\nYour researchers can now use research benches to begin exploring how to replicate the technology.</completedLetter>
				<completedLetterDef>PositiveEvent</completedLetterDef>

				<!-- Interactable -->

				<maintainProgress>true</maintainProgress>
				<activateTexPath>UI/Icons/Study</activateTexPath>

				<activateLabelString>Analyze...</activateLabelString>
				<activateDescString>Analyze the Kull's armor.</activateDescString>
				<jobString>Analyzing Kull armor</jobString>
				<guiLabelString>Choose who should analyze this</guiLabelString>
				<inspectString>Can be analyzed by a colonist.</inspectString>

				<targetingParameters>
					<canTargetBuildings>false</canTargetBuildings>
					<canTargetAnimals>false</canTargetAnimals>
					<canTargetMechs>false</canTargetMechs>
					<onlyTargetControlledPawns>true</onlyTargetControlledPawns>
				</targetingParameters>
			</li>
		</comps>
	</ThingDef>

