<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- Sarcophagus -->

	<JobDef>
		<defName>Rimgate_CarryToSarcophagus</defName>
		<driverClass>Rimgate.JobDriver_CarryToSarcophagus</driverClass>
		<reportString>carrying TargetA to TargetB.</reportString>
		<casualInterruptible>false</casualInterruptible>
		<carryThingAfterJob>true</carryThingAfterJob>
	</JobDef>

	<JobDef>
		<defName>Rimgate_PatientGoToSarcophagus</defName>
		<driverClass>Rimgate.JobDriver_PatientGoToSarcophagus</driverClass>
		<reportString>receiving treatment in TargetA.</reportString>
		<suspendable>false</suspendable>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_RescueToSarcophagus</defName>
		<driverClass>Rimgate.JobDriver_CarryToSarcophagus</driverClass>
		<reportString>rescuing TargetA to TargetB.</reportString>
		<casualInterruptible>false</casualInterruptible>
		<carryThingAfterJob>true</carryThingAfterJob>
	</JobDef>

	<!-- Cloning Pod -->

	<JobDef>
		<defName>Rimgate_EnterCloningPod</defName>
		<driverClass>Rimgate.JobDriver_EnterCloningPod</driverClass>
		<reportString>entering cloning pod.</reportString>
		<taleOnCompletion>EnteredCloningPod</taleOnCompletion>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_CloneOccupantGenes</defName>
		<driverClass>Rimgate.JobDriver_CloneOccupantGenes</driverClass>
		<reportString>cloning by genome only.</reportString>
		<casualInterruptible>false</casualInterruptible>
	</JobDef>

	<JobDef>
		<defName>Rimgate_CloneOccupantFull</defName>
		<driverClass>Rimgate.JobDriver_CloneOccupantFull</driverClass>
		<reportString>deep-scan cloning.</reportString>
		<casualInterruptible>false</casualInterruptible>
	</JobDef>

	<JobDef>
		<defName>Rimgate_CloneOccupantSoldier</defName>
		<driverClass>Rimgate.JobDriver_CloneOccupantSoldier</driverClass>
		<reportString>reforging.</reportString>
		<casualInterruptible>false</casualInterruptible>
	</JobDef>

	<JobDef>
		<defName>Rimgate_CloneReconstructDead</defName>
		<driverClass>Rimgate.JobDriver_CloneReconstructDead</driverClass>
		<reportString>reconstructing.</reportString>
		<casualInterruptible>false</casualInterruptible>
	</JobDef>

	<JobDef>
		<defName>Rimgate_CarryToCloningPod</defName>
		<driverClass>Rimgate.JobDriver_CarryToCloningPod</driverClass>
		<reportString>carrying TargetA to cloning pod.</reportString>
		<casualInterruptible>false</casualInterruptible>
		<taleOnCompletion>PutIntoCloningPod</taleOnCompletion>
	</JobDef>

	<JobDef>
		<defName>Rimgate_CarryCorpseToCloningPod</defName>
		<driverClass>Rimgate.JobDriver_CarryCorpseToCloningPod</driverClass>
		<reportString>carrying TargetA to cloning pod.</reportString>
		<casualInterruptible>false</casualInterruptible>
		<taleOnCompletion>PutIntoCloningPod</taleOnCompletion>
	</JobDef>

	<!-- Stargate -->

	<JobDef>
		<defName>Rimgate_EnterStargate</defName>
		<driverClass>Rimgate.JobDriver_EnterStargate</driverClass>
		<reportString>Entering TargetA.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_DialStargate</defName>
		<driverClass>Rimgate.JobDriver_DialStargate</driverClass>
		<reportString>Using TargetA.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_CloseStargate</defName>
		<driverClass>Rimgate.JobDriver_CloseStargate</driverClass>
		<reportString>closing stargate connection.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_ToggleIris</defName>
		<driverClass>Rimgate.JobDriver_ToggleIris</driverClass>
		<reportString>toggle stargate iris.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_BringToStargate</defName>
		<driverClass>Rimgate.JobDriver_BringToStargate</driverClass>
		<reportString>Bringing TargetA to TargetB.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_InstallIris</defName>
		<driverClass>Rimgate.JobDriver_InstallIris</driverClass>
		<reportString>Installing iris on TargetB.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<JobDef>
		<defName>Rimgate_DecodeGlyphs</defName>
		<driverClass>Rimgate.JobDriver_DecodeGlyphs</driverClass>
		<reportString>Decoding stargate glyphs.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

	<!-- Misc -->

	<JobDef>
		<defName>Rimgate_Toggle</defName>
		<driverClass>Rimgate.JobDriver_Toggle</driverClass>
		<reportString>toggling switch.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>

</Defs>