    <!-- ==================== Symbiote ==================== -->

    <ThingDef ParentName="BodyPartBase">
        <defName>Rimgate_SymbioteImplant</defName>
        <label>goa'uld symbiote</label>
        <description>A small, serpentine creature genetically engineered to enhance and sustain a
            host.</description>
        <descriptionHyperlinks>
            <RecipeDef>Rimgate_InstallSymbiote</RecipeDef>
        </descriptionHyperlinks>
        <techLevel>Neolithic</techLevel>
