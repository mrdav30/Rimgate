            <Mass>2</Mass>
            <MarketValue>800</MarketValue>
            <MaxHitPoints>100</MaxHitPoints>
        </statBases>
        <thingCategories>
            <li>BodyPartsBionic</li>
        </thingCategories>
        <techHediffsTags>
            <li>Rimgate_GoauldSymbiote_tag</li>
        </techHediffsTags>
    </ThingDef>

