			<SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">-0.3</SlaveSuppressionOffset>
			<PsychicSensitivityOffset>0.2</PsychicSensitivityOffset>
			<PsychicEntropyRecoveryRate>0.07</PsychicEntropyRecoveryRate>
		</equippedStatOffsets>
		<thingCategories Inherit="False">
			<li>Headgear</li>
		</thingCategories>
		<apparel>
			<wornGraphicPath>Things/Item/Apparel/Headgear/WraithCirclet/RGWraithCirclet</wornGraphicPath>
