	<!-- ========= Wraith communication device ========= -->

	<ThingDef ParentName="Rimgate_WraithArmorBase">
		<defName>Rimgate_WraithCommunicationDevice</defName>
		<label>Wraith communication device</label>
		<description>Wraith-built synaptic interface for coordinating mechs, hacking systems, and maybe ordering takeout from the hive mind.</description>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedBelt</unfinishedThingDef>
			<researchPrerequisite>Rimgate_WraithCommunicationDevice</researchPrerequisite>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Smith</effectWorking>
			<soundWorking>Recipe_Machining</soundWorking>
			<useIngredientsForColor>false</useIngredientsForColor>
			<skillRequirements>
				<Crafting>10</Crafting>
			</skillRequirements>
		</recipeMaker>
		<costList>
			<ComponentSpacer>2</ComponentSpacer>
			<Steel>25</Steel>
			<Plasteel>25</Plasteel>
			<Rimgate_Biomass>5</Rimgate_Biomass>
		</costList>
		<graphicData>
			<texPath>Things/Item/Apparel/Utility/RGWraithCommunicatonDevice</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<MarketValue>1500</MarketValue>
			<Mass>1.5</Mass>
			<WorkToMake>14000</WorkToMake>
			<Flammability>0.4</Flammability>
			<EquipDelay>2</EquipDelay>
		</statBases>
		<tradeability>Sellable</tradeability>
		<generateAllowChance>0.15</generateAllowChance>
		<equippedStatOffsets>
			<MechBandwidth MayRequire="Ludeon.RimWorld.Biotech">6</MechBandwidth>
			<MechControlGroups MayRequire="Ludeon.RimWorld.Biotech">1</MechControlGroups>
			<HackingSpeed>0.25</HackingSpeed>
			<HackingStealth>0.25</HackingStealth>
			<ResearchSpeed>0.25</ResearchSpeed>
		</equippedStatOffsets>
		<apparel>
			<careIfWornByCorpse>false</careIfWornByCorpse>
			<careIfDamaged>false</careIfDamaged>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<bodyPartGroups Inherit="false">
				<li>Waist</li>
			</bodyPartGroups>
			<layers Inherit="false">
				<li>Belt</li>
			</layers>
			<tags>
				<li>Rimgate_WraithUtility_tag</li>
			</tags>
		</apparel>
		<tickerType>Normal</tickerType>
		<thingCategories Inherit="False">
			<li>ApparelUtility</li>
		</thingCategories>
		<thingSetMakerTags>
			<li>RewardStandardLowFreq</li>
		</thingSetMakerTags>
		<comps>
			<li Class="Rimgate.CompProperties_AnalyzableResearchWhen">
				<analysisID>1000100</analysisID>
				<requiresResearchDef>StandardMechtech</requiresResearchDef>

				<!-- Analyzable -->

				<destroyedOnAnalyzed>true</destroyedOnAnalyzed>
				<analysisDurationHours>10</analysisDurationHours>
				<completedLetterLabel>Wraith comm device studied: {RESEARCH} unlocked</completedLetterLabel>
				<completedLetter>By studying the Wraith comm device, {PAWN_labelShort} has acquired the insight needed for {RESEARCH} research.\n\nWhen disconnected from its organic matrix, the unit dissolved into ash, its last signal a whisper of alien thought fading to static.\n\nHowever, your researchers can now use research benches to begin investigating the technology.</completedLetter>
				<completedLetterDef>PositiveEvent</completedLetterDef>

				<catastrophicFailureChance>0.25</catastrophicFailureChance>
				<catastrophicDestroysThing>true</catastrophicDestroysThing>
				<catastrophicExplosion>true</catastrophicExplosion>
				<catastrophicExplosionRadius>1.0</catastrophicExplosionRadius>
				<catastrophicExplosionDamage>5</catastrophicExplosionDamage>
				<catastrophicExplosionDamageDef>Bomb</catastrophicExplosionDamageDef>
				<catastrophicBlocksResearch>true</catastrophicBlocksResearch>

				<catastrophicLetterLabelKey>RG_AnalysisCatastrophicLabel</catastrophicLetterLabelKey>
				<catastrophicLetterTextKey>RG_AnalysisCatastrophicText</catastrophicLetterTextKey>

				<!-- Interactable -->

				<maintainProgress>true</maintainProgress>
				<activateTexPath>UI/Icons/Study</activateTexPath>

				<activateLabelString>Analyze...</activateLabelString>
				<activateDescString>Analyze the Wraith comm device.</activateDescString>
				<jobString>Analyzing Wraith comm device</jobString>
				<guiLabelString>Choose who should analyze this</guiLabelString>
				<inspectString>Can be analyzed by a colonist.</inspectString>

				<targetingParameters>
					<canTargetBuildings>false</canTargetBuildings>
					<canTargetAnimals>false</canTargetAnimals>
					<canTargetMechs>false</canTargetMechs>
					<onlyTargetControlledPawns>true</onlyTargetControlledPawns>
				</targetingParameters>
			</li>
		</comps>
	</ThingDef>

