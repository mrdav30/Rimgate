<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- =============== Naquadah ==================== -->

  <ThingDef ParentName="ResourceBase">
    <defName>Rimgate_Naquadah</defName>
    <label>Naquadah</label>
    <description>A dense, metallic mineral that hums faintly under stress and can power everything from hand weapons to planetary extinction events.\n\nOnce refined, it becomes Naquadria — exponentially more powerful, exponentially more likely to redecorate your map in plasma.\n\nHandle with insulated gloves and smug superiority.</description>
    <graphicData>
      <texPath>Things/Item/Resource/RGNaquadah</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <useHitPoints>false</useHitPoints>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Metal_Drop</soundDrop>
    <statBases>
      <MarketValue>80</MarketValue>
      <Mass>0.3</Mass>
      <StuffPower_Armor_Sharp>1.08</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0.54</StuffPower_Armor_Blunt>
      <StuffPower_Armor_Heat>0.9</StuffPower_Armor_Heat>
      <StuffPower_Insulation_Cold>3</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>0</StuffPower_Insulation_Heat>
      <SharpDamageMultiplier>1.1</SharpDamageMultiplier>
      <BluntDamageMultiplier>1.5</BluntDamageMultiplier>
    </statBases>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <stuffProps>
      <categories>
        <li>Metallic</li>
      </categories>
      <appearance>Metal</appearance>
      <isAirtight>true</isAirtight>
      <commonality>0</commonality>
      <constructEffect>ConstructMetal</constructEffect>
      <color>(114,114,114)</color>
      <soundImpactBullet>BulletImpact_Metal</soundImpactBullet>
      <soundMeleeHitSharp>MeleeHit_Metal_Sharp</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Metal_Blunt</soundMeleeHitBlunt>
      <soundImpactMelee>Pawn_Melee_Punch_HitBuilding_Metal</soundImpactMelee>
      <statFactors>
        <MaxHitPoints>2.0</MaxHitPoints>
        <Beauty>3.2</Beauty>
        <Flammability>0.2</Flammability>
        <DoorOpenSpeed>1</DoorOpenSpeed>
        <WorkToMake>2.5</WorkToMake>
        <WorkToBuild>2.5</WorkToBuild>
        <BedRestEffectiveness>0.85</BedRestEffectiveness>
        <MeleeWeapon_CooldownMultiplier>1</MeleeWeapon_CooldownMultiplier>
      </statFactors>
      <statFactorsQuality>
        <li>
          <stat>PsychicSensitivityFactor</stat>
          <awful>1.06</awful>
          <poor>1.08</poor>
          <normal>1.1</normal>
          <good>1.13</good>
          <excellent>1.15</excellent>
          <masterwork>1.17</masterwork>
          <legendary>1.20</legendary>
        </li>
      </statFactorsQuality>
      <statOffsetsQuality>
        <li MayRequireAnyOf="Ludeon.RimWorld.Royalty,Ludeon.RimWorld.Biotech">
          <stat>PsychicEntropyMaxOffset</stat>
          <awful>4</awful>
          <poor>6</poor>
          <normal>8</normal>
          <good>11</good>
          <excellent>13</excellent>
          <masterwork>15</masterwork>
          <legendary>18</legendary>
        </li>
        <li MayRequireAnyOf="Ludeon.RimWorld.Royalty,Ludeon.RimWorld.Biotech">
          <stat>PsychicEntropyRecoveryRateOffset</stat>
          <awful>0.04</awful>
          <poor>0.04</poor>
          <normal>0.04</normal>
          <good>0.04</good>
          <excellent>0.04</excellent>
          <masterwork>0.04</masterwork>
          <legendary>0.04</legendary>
        </li>
      </statOffsetsQuality>
    </stuffProps>
    <deepCommonality>6</deepCommonality>
    <deepCountPerPortion>45</deepCountPerPortion>
    <deepLumpSizeRange>
      <min>30</min>
      <max>40</max>
    </deepLumpSizeRange>
    <comps>
      <li Class="Rimgate.CompProperties_AnalyzableResearchWhen">
        <analysisID>1000800</analysisID>
        <requiresResearchDef>AdvancedFabrication</requiresResearchDef>
        <canStudyInPlace>false</canStudyInPlace>
        <destroyedOnAnalyzed>true</destroyedOnAnalyzed>
        <analysisDurationHours>8</analysisDurationHours>
        <maintainProgress>true</maintainProgress>

        <catastrophicFailureChance>0.25</catastrophicFailureChance>
        <catastrophicDestroysThing>true</catastrophicDestroysThing>
        <catastrophicExplosion>true</catastrophicExplosion>
        <catastrophicExplosionRadius>1.5</catastrophicExplosionRadius>
        <catastrophicExplosionDamage>10</catastrophicExplosionDamage>
        <catastrophicExplosionDamageDef>Bomb</catastrophicExplosionDamageDef>
        <catastrophicBlocksResearch>true</catastrophicBlocksResearch>

        <catastrophicLetterLabelKey>RG_AnalysisCatastrophicLabel</catastrophicLetterLabelKey>
        <catastrophicLetterTextKey>RG_AnalysisCatastrophicText</catastrophicLetterTextKey>

        <activateTexPath>UI/Icons/Study</activateTexPath>
        <activateLabelString>Analyze...</activateLabelString>
        <activateDescString>Analyze the Naquadah sample.</activateDescString>
        <jobString>Analyzing Naquadah sample</jobString>
        <guiLabelString>Choose who should analyze this</guiLabelString>
        <inspectString>Can be analyzed by a colonist.</inspectString>

        <completedLetterLabel>{RESEARCH} unlocked</completedLetterLabel>
        <completedLetter>{PAWN_labelShort} has successfully analyzed the Naquadah sample and unlocked {RESEARCH} research.\n\nThe residue flash-vaporized during testing, coating the lab in a faintly glowing dust.\n\nAll nearby Geiger counters have been advised to stop screaming.</completedLetter>
        <completedLetterDef>PositiveEvent</completedLetterDef>

        <targetingParameters>
          <canTargetBuildings>false</canTargetBuildings>
          <canTargetAnimals>false</canTargetAnimals>
          <canTargetMechs>false</canTargetMechs>
          <onlyTargetControlledPawns>true</onlyTargetControlledPawns>
        </targetingParameters>
      </li>
    </comps>
  </ThingDef>

  <!-- =============== Naquadria ==================== -->

  <ThingDef ParentName="ResourceBase">
    <defName>Rimgate_Naquadria</defName>
    <label>Naquadria</label>
    <description>a highly enriched, dangerously unstable derivative of Naquadah. Perfect for experiments that end in glorious explosions—or annihilating your base if you sneeze too hard.</description>
    <graphicData>
      <texPath>Things/Item/Resource/RGNaquadria</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <possessionCount>20</possessionCount>
    <tickerType>Normal</tickerType>
    <stackLimit>150</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Metal_Drop</soundDrop>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>800</MarketValue>
      <Mass>0.3</Mass>
      <Flammability>2.0</Flammability>
      <DeteriorationRate>1.0</DeteriorationRate>
    </statBases>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
    <thingSetMakerTags>
      <li>RewardStandardCore</li>
    </thingSetMakerTags>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <comps>
      <li Class="CompProperties_Explosive">
        <explosiveRadius>2.66</explosiveRadius>
        <explosiveDamageType>Flame</explosiveDamageType>
        <explosiveExpandPerStackcount>0.037</explosiveExpandPerStackcount>
        <startWickOnDamageTaken>
          <li>Flame</li>
        </startWickOnDamageTaken>
        <startWickHitPointsPercent>0.333</startWickHitPointsPercent>
        <preExplosionSpawnThingDef>Filth_Fuel</preExplosionSpawnThingDef>
        <preExplosionSpawnChance>1</preExplosionSpawnChance>
        <wickTicks>70~150</wickTicks>
      </li>
    </comps>
    <allowedArchonexusCount>150</allowedArchonexusCount>
  </ThingDef>

  <!-- ==================== Tretonin ==================== -->

  <ThingDef ParentName="MechSerumBase">
    <defName>Rimgate_Tretonin</defName>
    <label>Tretonin</label>
    <description>feeling sick, dying of alien withdrawals, or just done with having an immune system?\nTry Tretonin! It’s a miracle cure—if you don’t mind needing it forever.\nSide effects may include dependency and an unshakable desperation.</description>
    <graphicData>
      <texPath>Things/Item/Resource/RGTretonin</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <stackLimit>10</stackLimit>
    <statBases>
      <MarketValue>1300</MarketValue>
    </statBases>
    <thingCategories>
      <li>Drugs</li>
    </thingCategories>
    <ingestible>
      <drugCategory>Medical</drugCategory>
      <foodType>Fluid, Processed</foodType>
      <joyKind>Chemical</joyKind>
      <joy>0.2</joy>
      <baseIngestTicks>210</baseIngestTicks>
      <nurseable>false</nurseable>
      <preferability>NeverForNutrition</preferability>
      <maxNumToIngestAtOnce>1</maxNumToIngestAtOnce>
      <defaultNumToIngestAtOnce>1</defaultNumToIngestAtOnce>
      <ingestSound>Ingest_Drink</ingestSound>
      <ingestHoldOffsetStanding>
        <northDefault>
          <offset>(0.18,0,0)</offset>
        </northDefault>
      </ingestHoldOffsetStanding>
      <ingestCommandString>Drink {0}</ingestCommandString>
      <ingestReportString>Drinking {0}.</ingestReportString>
      <ingestHoldUsesTable>false</ingestHoldUsesTable>
      <outcomeDoers>
        <li Class="Rimgate.IngestionOutcomeDoer_HealAllDisease">
          <inclusions>
            <li>Rimgate_SymbioteWithdrawal</li>
          </inclusions>
          <!-- <forceRemove>
            <li>Rimgate_PrimtaInPouch</li>
            <li>Rimgate_SymbioteImplant</li>
          </forceRemove> -->
        </li>
      </outcomeDoers>
    </ingestible>
    <comps>
      <li Class="CompProperties_Drug">
        <chemical>Rimgate_TretoninChemical</chemical>
        <addictiveness>1.00</addictiveness>
        <needLevelOffset>0.9</needLevelOffset>
        <listOrder>1020</listOrder>
      </li>
      <li Class="CompProperties_UseEffectPlaySound">
        <soundOnUsed>MechSerumUsed</soundOnUsed>
      </li>
      <li Class="CompProperties_UseEffectDestroySelf" />
    </comps>
    <thingSetMakerTags>
      <li>RewardStandardCore</li>
    </thingSetMakerTags>
  </ThingDef>

  <RecipeDef>
    <defName>Rimgate_AdministerTretonin</defName>
    <label>administer Tretonin</label>
    <description>Administer Tretonin.</description>
    <workerClass>Recipe_AdministerUsableItem</workerClass>
    <jobString>Administering Tretonin.</jobString>
    <targetsBodyPart>false</targetsBodyPart>
    <anesthetize>false</anesthetize>
    <workAmount>600</workAmount>
    <dontShowIfAnyIngredientMissing>true</dontShowIfAnyIngredientMissing>
    <surgeryOutcomeEffect IsNull="True" />
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Rimgate_Tretonin</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Rimgate_Tretonin</li>
      </thingDefs>
    </fixedIngredientFilter>
  </RecipeDef>

  <!-- ==================== Meat ==================== -->

  <ThingDef ParentName="OrganicProductBase">
    <defName>Rimgate_CocoonedMeat</defName>
    <label>cocooned meat</label>
    <description>some meat, now lovingly wrapped in alien silk for freshness.\nSmells faintly of despair and industrial solvents. At least it won’t rot!</description>
    <graphicData>
      <texPath>Things/Item/Resource/RGCocoonedMeat</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <ingestible>
      <foodType>Kibble</foodType>
      <preferability>MealFine</preferability>
      <maxNumToIngestAtOnce>1</maxNumToIngestAtOnce>
      <defaultNumToIngestAtOnce>1</defaultNumToIngestAtOnce>
      <ingestEffect>EatMeat</ingestEffect>
      <ingestSound>RawMeat_Eat</ingestSound>
      <tasteThought>AteRawFood</tasteThought>
    </ingestible>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <DeteriorationRate Inherit="False">0</DeteriorationRate>
      <Nutrition>0.20</Nutrition>
      <FoodPoisonChanceFixedHuman>0.02</FoodPoisonChanceFixedHuman>
      <MarketValue>2</MarketValue>
      <MaxHitPoints>60</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <Mass>0.25</Mass>
    </statBases>
    <thingCategories>
      <li>MeatRaw</li>
    </thingCategories>
  </ThingDef>

  <!-- =============== Biomass ==================== -->

  <ThingDef ParentName="ResourceBase">
    <defName>Rimgate_Biomass</defName>
    <label>Biomass</label>
