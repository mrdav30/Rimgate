	<!-- ====== Goa'uld Helmet ====== -->

	<ThingDef ParentName="Rimgate_GoauldHeadgearMakerBase">
		<defName>Rimgate_JaffaHelmet</defName>
		<label>Jaffa helmet</label>
		<description>lightweight ceremonial headgear favored by Jaffa cannon fodder. Offers mild protection and maximum intimidation—mostly to unarmed colonists and small wildlife.</description>
		<costList>
			<Steel>25</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
			<Rimgate_Naquadah>1</Rimgate_Naquadah>
		</costList>
		<graphicData>
			<texPath>Things/Item/Apparel/Headgear/JaffaHelm/RGJaffaHelmet</texPath>
		</graphicData>
		<statBases>
			<MarketValue>750</MarketValue>
			<MaxHitPoints>80</MaxHitPoints>
			<WorkToMake>1000</WorkToMake>
			<Mass>0.9</Mass>
			<ArmorRating_Sharp>0.56</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.24</ArmorRating_Blunt>
			<ArmorRating_Heat>0.16</ArmorRating_Heat>
			<Flammability>0.4</Flammability>
			<EquipDelay>2</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<ShootingAccuracyPawn>-0.05</ShootingAccuracyPawn>
			<PainShockThreshold>0.05</PainShockThreshold>
			<SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">-0.1</SlaveSuppressionOffset>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>UpperHead</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Item/Apparel/Headgear/JaffaHelm/RGJaffaHelmet</wornGraphicPath>
			<tags>
				<li>Rimgate_GoauldApparel_tag</li>
			</tags>
		</apparel>
	</ThingDef>

