	<!-- ============ Jaffa Helm Base ============ -->

	<ThingDef Name="Rimgate_GoauldHeadgearBase" ParentName="ApparelBase" Abstract="True">
		<techLevel>Spacer</techLevel>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>RGBA(1,1,1,1)</only>
				</li>
			</options>
		</colorGenerator>
		<tradeTags>
			<li>Rimgate_GoauldApparelTrade_tag</li>
			<li>HiTechArmor</li>
			<li>ExoticMisc</li>
		</tradeTags>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<apparel>
			<parentTagDef>ApparelHead</parentTagDef>
			<layers>
				<li>Overhead</li>
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
			<useDeflectMetalEffect>true</useDeflectMetalEffect>
		</apparel>
		<thingCategories>
			<li>ArmorHeadgear</li>
		</thingCategories>
	</ThingDef>

	<ThingDef Name="Rimgate_GoauldHeadgearMakerBase" ParentName="Rimgate_GoauldHeadgearBase" Abstract="True">
