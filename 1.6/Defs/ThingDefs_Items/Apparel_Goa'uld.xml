	<!-- ====== Goa'uld Horus Helmet ====== -->

	<ThingDef ParentName="Rimgate_GuardHeadgearBase">
		<defName>Rimgate_HorusHelmet</defName>
		<label>Horus helmet</label>
		<description>the signature helm of Ra’s elite peacocks—er, warriors. High shine, moderate armor, and excellent against everything but bullets. Just don’t try to look up stairs in it.</description>
		<graphicData>
			<texPath>Things/Item/Apparel/Headgear/GuardHelm/RGHorusHelmet</texPath>
		</graphicData>
		<apparel>
			<wornGraphicPath>Things/Item/Apparel/Headgear/GuardHelm/RGHorusHelmet</wornGraphicPath>
			<drawData>
				<dataNorth>
					<layer>72</layer>
					<!--across, depth, height-->
					<offset>(0, 1, 0)</offset>
				</dataNorth>
			</drawData>
		</apparel>
		<comps>
			<li Class="Rimgate.CompProperties_AlternateApparel">
				<attachedHeadgearDef>Rimgate_HorusHelmetOpen</attachedHeadgearDef>
				<toggleUiIconPath>Things/Item/Apparel/Headgear/GuardHelm/RGGuardHelmetOpen</toggleUiIconPath>
				<isClosedState>true</isClosedState>
			</li>
		</comps>
	</ThingDef>

