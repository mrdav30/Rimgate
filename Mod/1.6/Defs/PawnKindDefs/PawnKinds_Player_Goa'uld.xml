<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <PawnKindDef Name="Rimgate_GoauldPlayerBase" Abstract="True">
    <race>Human</race>
    <defaultFactionType>Rimgate_GoauldPlayerColony</defaultFactionType>
    <combatPower>40</combatPower>
    <isFighter>true</isFighter>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <apparelIgnorePollution MayRequire="Ludeon.RimWorld.Biotech">true</apparelIgnorePollution>
    <forceNormalGearQuality>true</forceNormalGearQuality>
    <initialWillRange>0~0</initialWillRange>
    <humanPregnancyChance>0.06</humanPregnancyChance>
    <initialResistanceRange>13~21</initialResistanceRange>
  </PawnKindDef>

  <PawnKindDef ParentName="Rimgate_GoauldPlayerBase">
    <defName>Rimgate_GoauldPlayer</defName>
    <label>free Jaffa</label>
    <chemicalAddictionChance>0</chemicalAddictionChance>
    <xenotypeSet>
      <xenotypeChances>
        <Rimgate_GoauldJaffa>999</Rimgate_GoauldJaffa>
      </xenotypeChances>
    </xenotypeSet>
    <backstoryFilters>
      <li>
        <categories>
          <li>Rimgate_GoauldChildBackstory</li>
          <li>Rimgate_FirstPrimeBackstory</li>
          <li>Rimgate_TokraBackstory</li>
          <li>Rimgate_GoauldAdultBackstory</li>
        </categories>
      </li>
    </backstoryFilters>
    <useFactionXenotypes>false</useFactionXenotypes>
    <minGenerationAge>19</minGenerationAge>
    <maxGenerationAge>110</maxGenerationAge>
    <gearHealthRange>0.7~2.3</gearHealthRange>
    <apparelAllowHeadgearChance>0.5</apparelAllowHeadgearChance>
    <apparelMoney>1200~2400</apparelMoney>
    <apparelTags>
      <li>Neolithic</li>
      <li>IndustrialBasic</li>
      <li>Rimgate_GoauldApparel_tag</li>
    </apparelTags>
    <weaponMoney>1200~2500</weaponMoney>
    <weaponTags>
      <li>Rimgate_GoauldWeapons_tag</li>
    </weaponTags>
    <techHediffsChance>1</techHediffsChance>
    <techHediffsMoney>1200</techHediffsMoney> <!-- Just enough to "pay" for the implant -->
    <techHediffsTags>
      <li>Rimgate_JaffaForeheadSymbol_tag</li>
    </techHediffsTags>
    <startingHediffs>
      <li>
        <def>Rimgate_SymbiotePouch</def>
      </li>
      <li>
        <def>Rimgate_PrimtaInPouch</def>
      </li>
    </startingHediffs>
  </PawnKindDef>

  <PawnKindDef ParentName="Rimgate_GoauldPlayerBase">
    <defName>Rimgate_TokraPlayer</defName>
    <label>Tok'ra</label>
    <combatPower>60</combatPower>
    <isFighter>true</isFighter>
    <minGenerationAge>19</minGenerationAge>
    <maxGenerationAge>110</maxGenerationAge>
    <gearHealthRange>0.7~2.3</gearHealthRange>
    <apparelAllowHeadgearChance>0.8</apparelAllowHeadgearChance>
    <apparelMoney>1800~3000</apparelMoney>
    <apparelTags>
      <li>Neolithic</li>
      <li>IndustrialBasic</li>
      <li>Rimgate_GoauldApparel_tag</li>
    </apparelTags>
    <weaponMoney>2000~4000</weaponMoney>
    <weaponTags>
      <li>Rimgate_GoauldWeapons_tag</li>
    </weaponTags>
    <techHediffsMoney>0</techHediffsMoney>
    <startingHediffs>
      <li>
        <def>Rimgate_SymbioteImplant</def>
      </li>
    </startingHediffs>
    <backstoryFilters>
      <li>
        <categoriesAdulthood>
          <li>Rimgate_TokraBackstory</li>
        </categoriesAdulthood>
      </li>
    </backstoryFilters>
  </PawnKindDef>

  <PawnKindDef ParentName="Rimgate_GoauldPlayerBase">
    <defName>Rimgate_GoauldPlayerSlave</defName>
    <label>slave</label>
    <combatPower>25</combatPower>
    <isFighter>false</isFighter>
    <gearHealthRange>0.2~2.0</gearHealthRange>
    <apparelTags>
      <li>Tribal</li>
      <li>Neolithic</li>
      <li>IndustrialBasic</li>
    </apparelTags>
    <chemicalAddictionChance>0.5</chemicalAddictionChance>
    <apparelMoney>270~900</apparelMoney>
    <weaponMoney>270~900</weaponMoney>
    <weaponTags>
      <li>Tribal</li>
    </weaponTags>
    <backstoryFiltersOverride>
      <li>
        <categoriesChildhood>
          <li>Tribal</li>
        </categoriesChildhood>
        <categoriesAdulthood>
          <li>Rimgate_SlaveBackstory</li>
        </categoriesAdulthood>
      </li>
    </backstoryFiltersOverride>
  </PawnKindDef>

</Defs>