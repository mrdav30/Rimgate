    <Rimgate.BiomaterialRecoveryDef Name="Rimgate_RecoveryBase" Abstract="True">
        <descriptionHyperlinks>
            <ThingDef>Rimgate_BiomaterialProcurementKit</ThingDef>
        </descriptionHyperlinks>
        <uiIconPath>Things/Item/Resource/ProcurementKit</uiIconPath>

        <requiredKit>Rimgate_BiomaterialProcurementKit</requiredKit>

        <workAmount>2000</workAmount>
        <workSpeedStat>MedicalOperationSpeed</workSpeedStat>

        <skillRequirements>
            <Medicine>6</Medicine>
            <Intellectual>6</Intellectual>
        </skillRequirements>

        <workSkill>Medicine</workSkill>
        <workSkillLearnFactor>0.02</workSkillLearnFactor>
        <effectWorking>Surgery</effectWorking>
        <soundWorking>Recipe_Surgery</soundWorking>

        <successChance>1</successChance>
        <successChanceStat>MedicalSurgerySuccessChance</successChanceStat>
    </Rimgate.BiomaterialRecoveryDef>

