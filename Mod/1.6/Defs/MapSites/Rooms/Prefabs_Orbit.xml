<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <!-- DwarfGate -->

    <PrefabDef>
        <defName>Rimgate_Exterior_OrbitalDwarfgate</defName>

        <size>(5,6)</size>
        <rotations>North</rotations>
        <things>
            <Rimgate_Dwarfgate>
                <position>(2,0,3)</position>
            </Rimgate_Dwarfgate>
            <Rimgate_DialHomeDevice>
                <position>(4,0,1)</position>
            </Rimgate_DialHomeDevice>
        </things>
        <terrain>
            <AncientTile>
                <rects>
                    <li>(0,0,4,4)</li>
                </rects>
            </AncientTile>
        </terrain>
    </PrefabDef>

    <!-- Loot room -->

    <PrefabDef>
        <defName>Rimgate_Exterior_OrbitalSupplyRoom</defName>
        <size>(8,9)</size>
        <things>
            <AncientSealedCrate>
                <chance>0.85</chance>
                <position>(6,0,2)</position>
            </AncientSealedCrate>
            <Rimgate_SupplyCrateGreen>
                <chance>0.85</chance>
                <position>(1,0,2)</position>
            </Rimgate_SupplyCrateGreen>
            <Rimgate_SupplyCrateYellow>
                <chance>0.85</chance>
                <position>(6,0,6)</position>
            </Rimgate_SupplyCrateYellow>
            <Rimgate_SupplyCrateOrange>
                <chance>0.85</chance>
                <position>(4,0,4)</position>
            </Rimgate_SupplyCrateOrange>
            <Rimgate_SupplyCrateRed>
                <chance>0.85</chance>
                <position>(5,0,7)</position>
            </Rimgate_SupplyCrateRed>
            <Rimgate_SupplyCrateBlue>
                <chance>0.85</chance>
                <position>(2,0,5)</position>
            </Rimgate_SupplyCrateBlue>
            <RubblePile>
                <chance>0.75</chance>
                <rects>
                    <li>(5,5,6,6)</li>
                    <li>(4,6,4,6)</li>
                </rects>
            </RubblePile>
            <Rimgate_ForceFieldDoor>
                <positions>
                    <li>(4,0,1)</li>
                    <li>(3,0,7)</li>
                </positions>
            </Rimgate_ForceFieldDoor>
            <OrbitalAncientFortifiedWall>
                <rects>
                    <li>(3,1,3,1)</li>
                    <li>(5,1,5,1)</li>
                    <li>(0,1,2,1)</li>
                    <li>(6,1,7,1)</li>
                    <li>(0,2,0,7)</li>
                    <li>(7,2,7,7)</li>
                    <li>(1,4,2,4)</li>
                    <li>(5,4,6,4)</li>
                    <li>(1,7,2,7)</li>
                    <li>(4,7,6,7)</li>
                </rects>
            </OrbitalAncientFortifiedWall>
        </things>
    </PrefabDef>

    <!-- Loot -->

    <PrefabDef>
        <defName>Rimgate_SupplyCrateGreen</defName>
        <size>(1,1)</size>
        <things>
            <Rimgate_SupplyCrateGreen>
            </Rimgate_SupplyCrateGreen>
        </things>
    </PrefabDef>

    <PrefabDef>
        <defName>Rimgate_SupplyCrateYellow</defName>
        <size>(1,1)</size>
        <things>
            <Rimgate_SupplyCrateYellow>
            </Rimgate_SupplyCrateYellow>
        </things>
    </PrefabDef>

    <PrefabDef>
        <defName>Rimgate_SupplyCrateOrange</defName>
        <size>(1,1)</size>
        <things>
            <Rimgate_SupplyCrateOrange>
            </Rimgate_SupplyCrateOrange>
        </things>
    </PrefabDef>

    <PrefabDef>
        <defName>Rimgate_SupplyCrateRed</defName>
        <size>(1,1)</size>
        <things>
            <Rimgate_SupplyCrateRed>
            </Rimgate_SupplyCrateRed>
        </things>
    </PrefabDef>

    <PrefabDef>
        <defName>Rimgate_SupplyCrateBlue</defName>
        <size>(1,1)</size>
        <things>
            <Rimgate_SupplyCrateBlue>
            </Rimgate_SupplyCrateBlue>
